{
  "name": "@advjs/plugin-pominis",
  "type": "module",
  "version": "0.1.1",
  "description": "Pominis Adapter Plugin for ADV.JS",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MPL-2.0",
  "exports": {
    ".": "./dist/index.mjs",
    "./client": "./dist/client/index.mjs",
    "./*": "./*"
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "npm run build",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "release": "bumpp --no-commit --no-push --no-tag && pnpm -r publish --tag beta --no-git-checks"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "consola": "catalog:cli"
  },
  "devDependencies": {
    "advjs": "workspace:*",
    "unbuild": "catalog:build"
  }
}
